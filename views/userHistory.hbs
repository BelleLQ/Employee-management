<div class="row">
    <div class="col-md-12">
          <h2>WEB322 {{session.user.userName}} ({{session.user.email}}) History</h2>
          <hr />
          <div class="table-responsive">
            <table class="table">
            <thead>
                <tr>
                    <th>Login Date/Time</th>
                    <th>Client Information</th>
                </tr>
            </thead>
            <tbody>
                    {{#each session.user.loginHistory}}
                    <tr>
                        <td>{{this.dateTime}}</a></td>
                        <td>{{this.userAgent}}</a></td>
                    </tr>
                    {{/each}}
                </tbody>
            </table>
        </div>
    </div>
    {{#unless departments}}
    <div class="col-md-12 text-center">
            <strong>{{message}}</strong>
    </div>
    {{/unless}}
</div>
